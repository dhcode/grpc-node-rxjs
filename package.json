{
  "name": "grpc-node-rxjs",
  "version": "0.0.1",
  "description": "ReactiveX wrapper for grpc-node TypeScript projects",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "build-ts": "tsc",
    "prepack": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "generate-proto-typings": "ts-node src/generateTs.ts test-res/testservice.proto -o test-res/grpc-namespaces.ts"
  },
  "bin": {
    "grpc-node-rxjs": "./bin/grpc-node-rxjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhcode/grpc-node-rxjs.git"
  },
  "keywords": [
    "grpc",
    "node",
    "typescript",
    "rxjs"
  ],
  "author": "Dominik Herbst",
  "license": "MIT",
  "dependencies": {
    "grpc": "^1.8.4",
    "protobufjs": "^6.8.4",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.4",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
